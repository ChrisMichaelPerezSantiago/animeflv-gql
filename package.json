{
  "name": "animeflv-gql",
  "version": "1.0.0",
  "description": "Animeflv GQL is an alternative custom animeflv API that complements GraphQL for queries, you can access all the data and catalog of the animeflv.net website. You can enjoy all the content with Spanish subtitles and the latest in the anime world for free.",
  "author": {
    "name": "Chris Michael",
    "email": "chrisperezsantiago1@gmail.com",
    "url": "http://personal-porfolio.chrismichael.now.sh",
    "social": {
      "github": "https://github.com/ChrisMichaelPerezSantiago",
      "twitter": "https://twitter.com/Chris5855M"
    }
  },
  "contributors": [
    {
      "name": "Chris Michael",
      "url": "https://github.com/ChrisMichaelPerezSantiago",
      "reason": "Project Leader, and Developer"
    }
  ],
  "bugs": {
    "url": "https://github.com/ChrisMichaelPerezSantiago/animeflv-gql/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisMichaelPerezSantiago/animeflv-gql.git"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/graphql": "^6.5.4",
    "@nestjs/microservices": "^6.11.6",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/websockets": "^6.11.6",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.5",
    "apollo-server-express": "^2.10.0",
    "apollo-server-fastify": "^2.10.0",
    "axios": "^0.19.2",
    "bufferutil": "^4.0.1",
    "cache-manager": "^2.11.1",
    "class-transformer": "^0.2.3",
    "encoding": "^0.1.12",
    "fast-json-stringify": "^1.16.3",
    "fastify": "^2.12.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.6",
    "grpc": "^1.24.2",
    "kafkajs": "^1.12.0",
    "mqtt": "^3.0.0",
    "nats": "^1.4.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "uglify-es": "^3.3.9",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.1",
    "@types/microsoft__typescript-etw": "^0.1.0",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "25.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
